{
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.21.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prestart": "react-scripts build",
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "cypress:open": "cypress open",
    "cy:e2e": "cypress open --e2e --browser chrome --config baseUrl=http://localhost:3000",
    "dev:e2e": "start-test start 3000 cy:e2e",
    "test": "react-scripts test",
    "test:debug": "react-scripts --insepct-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "cy:run": "cypress run --e2e --browser chrome --config baseUrl=http://localhost:3000",
    "e2e": "start-test start 3000 cy:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.96",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.18.4",
    "cypress": "^13.7.2",
    "cypress-watch-and-reload": "^1.10.13",
    "msw": "^1.3.3",
    "start-server-and-test": "^2.0.3"
  }
