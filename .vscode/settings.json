{
  "editor.tabSize": 2,
  "[csharp]": {
    "editor.tabSize": 4
  },
  "typescript.tsdk": "./WeatherApp.UI/node_modules/typescript/lib",
  "files.associations": {
    "*.json": "jsonc"
  },
  "files.exclude": {
    "**/obj": true,
    "**/node_modules": true
  },
  "eslint.workingDirectories": [
    {
      "directory": "WeatherApp.UI",
      "changeProcessCWD": true
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "omnisharp.enableEditorConfigSupport": true,
  "dotnet.defaultSolution": "WeatherApp.sln",
  "appService.zipIgnorePattern": ["node_modules{,/**}", ".vscode{,/**}"]
}
